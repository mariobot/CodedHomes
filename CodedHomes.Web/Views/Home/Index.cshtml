@model IQueryable<CodedHomes.Models.Home>

@{
    ViewBag.Title = "Home Page";
}

@if (Model != null)
{
    <div class="container-fluid">
        <div class="row">
            <div class="col-md-3"></div>
            <div class="col-md-3">
                <table class="table table-bordered table-responsive">
                    <tr>
                        <td></td>
                        <td></td>
                        <td></td>
                        <td></td>
                        <td></td>
                        <td></td>
                        <td></td>
                        <td></td>
                        <td></td>
                        <td></td>
                        <td></td>
                        <td></td>
                    </tr>
                    @foreach (var house in Model)
                    {
                        <tr>
                            <td>@house.Id</td>
                            <td>@house.Description</td>
                            <td>@house.SquareFeet</td>
                            <td>@house.StreetAdress</td>
                            <td>@house.StreetAdress2</td>
                            <td>@house.ZipCode</td>
                            <td>@house.Price</td>
                            <td>@house.ImageName</td>
                            <td>@house.City</td>
                            <td>@house.Bedrooms</td>
                            <td>@house.Bathrooms</td>
                            <td>@house.CreatedOn</td>
                            <td>@house.ModifiedOn</td>
		    </tr>
                    }
                </table>
            </div>
            <div class="col-md-3"></div>
            <div class="col-md-3"></div>
        </div>
    </div>

}

